                                    <table  class="table table-bordered table-hover table-striped">
                                        <thead>
                                            <tr>
                                                <th style="text-align:center">Clients</th>
                                                <th style="text-align:center">Date signature du devis</th>
                                                <th style="text-align:center">Date prévisionnelle de fin de projet</th>
                                                <th style="text-align:center">Prix de la prestation</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                <% @projets.each do |projet| %>


                                <% if projet.date_avance_2 %>
                                    <% d=  projet.date_avance_2 %>                                
                                <% end %>
                                           

                      <% if projet.date_avance_4 %>

                      <% dfin= projet.date_avance_4 %>
                      <% drenouve= date_format(dfin+ 365.days) %>
                      <% dwarning= date_format(dfin+ 330.days) %>


                         <% if date_format(Time.now) >= dwarning %>
                           <tr class="danger">
                            <% else %>
                            <tr class="success">
                         <% end %>


                                  <td style="text-align:center"><%= projet.domaine %></td>
                                                

                                                <td style="text-align:center" colspan="2">
                                                           Le domaine devra être renouvelé avant le <%= drenouve %></td> 

                                                <td style="text-align:center">
                                                    <% if projet.tarif %>
                                                    <%= projet.tarif %> €</td>
                                                    <% else %>
                                                    <%= 'Aucun prix fixé' %>
                                                    <% end %>

                     <% else %>
                             <tr>


                                                  <td style="text-align:center"><%= projet.domaine %></td>
                                                
                                                    <% if projet.date_avance_2 %>
                                                            <td style="text-align:center">
                                                           <%= date_format(d) %>  </td>
                                                             <td style="text-align:center">
                                                           <%= date_format(d+ 40.days) %></td>
                                                            <% else %>
                                                            <td style="text-align:center"colspan='2'>
                                                            <%= 'Devis non validé' %>
                                                            </td>
                                                    <% end %>
                                               
                                                <td style="text-align:center">
                                                    <% if projet.tarif %>
                                                    <%= projet.tarif %> €</td>
                                                    <% else %>
                                                    <%= 'Aucun prix fixé' %>
                                                    <% end %>
                      <% end %>

                              

                                            </tr>
                                <% end %>

                                        </tbody>
                                    </table>
